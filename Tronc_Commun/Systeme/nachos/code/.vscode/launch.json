{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Nachos",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/userprog/nachos",
        "args": ["-rs", "123", "-x",  "test/makethreads"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
      }
    ]
  }